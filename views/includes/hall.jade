#games
    #games-menu.brown4.menubar
        a#g-matchs-games.menulink(href='#')= lingua.matchs 
        a#g-db-games.menulink(href='#')= lingua.database
        a#g-sgf-games.menulink(href='#') SGF
    #matchs-games.gamesscreen
        table#matchs-gameslist
    #db-games.gamesscreen
        p: a#refreshlist.linkbutton.black(href='#')= lingua.refresh
        table#db-gameslist
    #sgf-games.gamesscreen
        p: form#load-sgf.box.brown3(onsubmit='return false;')
            p: label= lingua.selectsgf
            #sgf-select.selectbox.link.yellow
            input#sgf-select-file(type='file', name='sgffile')
            p: input.linkbutton.black(type='submit', value= lingua.loadsgf)
            p: div#errorinvalid.box.red= lingua.errorinvalid
        p: form#create-sgf.box.brown3(onsubmit='return false;')
            p: label= lingua.size
            .selectbox.yellow
                .selectvalue 19
                select.link.select(name='size')
                    option(value='9') 9
                    option(value='13') 13
                    option(value='19', selected) 19
            p: label= lingua.rules
            .selectbox.yellow
                .selectvalue= lingua.japanese
                select.link.select(name='rules')
                    option(value='japanese', selected)= lingua.japanese
            p: input.linkbutton.black(type='submit', value= lingua.createsgf)
